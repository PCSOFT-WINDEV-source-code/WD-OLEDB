  procedures :
   -
     name : ModifyWindowMode
     internal_properties : CAAAAAgAAAB/eSdVka2tTlI9wME5ZIHQsjUK+s2GMXGm1Q7vMxSPuq8sDjyYzuFWGYLIJ9M7VH8L+90G2Qk9s5bETudKBSfVp3U2SYTqdg8ZfYISJ7SMowQmS2beSMoOCqw6ry1meW41/EJr0qosGapG7g==
     procedure_id : 1230901308799955735
     type_code : 14
     code : |1-
      // Modify the window mode
      // In:WindowMode=window opening mode:
      //				- "Browse"	Browse all the records
      //								with navigation buttons
      //				- "Modif"		Modify the current record
      //				- "Creation"	Creation of a new record
      Procedure ModifyWindowMode(WindowMode)
      
      // stores the window mode
      gsWindowMode=WindowMode
      
      // switch to browse mode
      IF WindowMode=<§$0000§> THEN
      	// the 'browse' buttons are enabled
      	GP_BROWSE..Visible=True
      	// buttons of modification mode 
      	GP_MODIF..Visible=False
      	// position on the first record
      	HReadFirst(Customers,CustomerCode)
      ELSE
      	// buttons for the browse mode 
      	GP_BROWSE..Visible=False
      	// buttons of modification mode are enabled
      	GP_MODIF..Visible=True
      	// if switch to creation mode
      	IF WindowMode=<§$0003§> THEN
      		// clears the controls
      		HReset(Customers)
      		EDT_CustomerCode..State=Active
      	END
      END
      
      // transfer the file buffer to the window controls
      FileToScreen()
     type : 458752
   -
     name : CheckModification
     internal_properties : CAAAAAgAAADLbz0MsybqZTDb46Rn+0AzZXD/piF0NWPdOyTE1RqgtcbyMIIGTWrw3PIBlKGMQ8Wbp55lHGA1YwpbOFQNDoFk/0eRSoU5rBqvWhuyeaYyOiHSd8dDc8Q=
     procedure_id : 1230901308800021271
     type_code : 14
     code : |1+
      // Check the form modifications
      // If the form was modified, ask the user
      // for confirmation
      // Modifies the record if confirmation// Automatic process of the errors: display the error and resume the input
      //
      // Automatic process of exceptions:   run CASE EXCEPTION:
      //
      
      Procedure CheckModification()
      
      // if a control was modified in the form
      IF MyWindow..Modified THEN
      	// indicates that the form is no longer modified
      	MyWindow..Modified=False
      
      	IF YesNo(<§$0004§>) THEN
      		// transfer controls to file buffer
      		ScreenToFile()
      		// modify the record
      		HModify(Customers)
      
      	END
      END
      
      CASE EXCEPTION:
      	DisplayError(ExceptionInfo(errMessage))
      	ReturnToCapture()
     type : 458752

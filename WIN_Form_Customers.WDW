     code : |1-
      // Open the Form window
      // In:OpeningMode=window opening mode:
      //				- "Browse"	Browse all the records
      //								with navigation buttons
      //				- "Modif"		Modify the current record
      //				- "Creation"	Creation of a new record
      Procedure WIN_Form_Customers(gsOpeningMode=<§$0000§>)
      
      GLOBAL
      	gsWindowMode is string		// window mode
      
      
      // enable the controls according to the window mode
      ModifyWindowMode(gsOpeningMode)
      
      // if the form is opened in browse mode while the file contains no record
      // automatically switches to creation mode
      IF gsOpeningMode=<§$0000§> AND HOut(Customers) THEN
      	// opens a dialog box to inform the user
      	Info(<§$0001§>)
      	// runs the click code of the New button to switch to edit mode
      	ExecuteProcess(BTN_New,trtClick)
      END

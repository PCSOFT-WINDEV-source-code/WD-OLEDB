        code : |1-
         // ask for confirmation
         IF YesNo(<§$0008§>) THEN
         	// deletion
         	HDelete(Customers)
         
         
         	// read the next record
         	HReadNext(Customers,CustomerCode)
         
         	// if the deleted record was the last record
         	IF HOut() THEN
         		// read the last record
         		HReadLast(Customers,CustomerCode)
         
         
